{
  "name": "@buka/nestjs-config",
  "version": "2.1.1",
  "description": "An easy to use nestjs config module",
  "main": "dist/umd/src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/esm/src/index.d.ts",
  "packageManager": "pnpm@9.3.0",
  "scripts": {
    "build": "npm run clean && ./build/build.sh",
    "clean": "rm -rf ./dist/*",
    "dev": "npm run clean && ./build/watch.sh",
    "prepare": "ts-patch install -s && is-ci || husky",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "test": "jest"
  },
  "keywords": [
    "nestjs",
    "nest",
    "module",
    "config",
    "configuration",
    "nconf",
    "dotenv",
    "env",
    "environment"
  ],
  "author": "Val.istar.Guo <val.istar.guo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/buka-lnc/npm.nestjs-config/#readme",
  "bugs": {
    "url": "https://github.com/buka-lnc/npm.nestjs-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buka-lnc/npm.nestjs-config.git"
  },
  "devDependencies": {
    "@buka/eslint-config": "^1.6.4",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@nestjs/common": "^10.3.9",
    "@nestjs/testing": "^10.3.9",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/node": "^20.14.2",
    "@types/object-path": "^0.11.4",
    "@types/ramda": "^0.30.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "memfs": "^4.9.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "change-case-all": "^2.1.0",
    "dotenv": "^16.4.5",
    "object-path": "^0.11.8",
    "ramda": "^0.30.1",
    "type-fest": "^4.20.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10",
    "class-transformer": "*",
    "class-validator": "*"
  }
